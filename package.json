{
  "name": "ai-research-report-generator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --dir . --max-warnings 0",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate --schema=./packages/lib/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./packages/lib/prisma/schema.prisma",
    "prisma:seed": "npm run seed",
    "prisma:studio": "prisma studio --schema=./packages/lib/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} packages/lib/prisma/seed.ts",
    "format": "prettier --write ."
  },
  "prisma": {
    "schema": "./packages/lib/prisma/schema.prisma",
    "seed": "ts-node packages/lib/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "clsx": "^2.1.1",
    "next": "15.5.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.9",
    "prettier": "^3.8.1",
    "prisma": "^5.15.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
